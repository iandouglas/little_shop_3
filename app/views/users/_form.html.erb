<%= form_for @user do |f| %>

<p><%= f.label :username %></p>
<p><%= f.text_field :username %></p>

<p><%= f.label :password %></p>
<p><%= f.password_field :password %></p>

<p><%= f.label :password_confirmation %></p>
<p><%= f.password_field :password_confirmation %></p>

<p><%= f.label :address %></p>
<p><%= f.text_field :address %></p>

<p><%= f.label :city %></p>
<p><%= f.text_field :city %></p>

<p><%= f.label :state %></p>
<p><%= f.text_field :state %></p>

<p><%= f.label :zip_code %></p>
<p><%= f.number_field :zip_code %></p>

<p><%= f.label :email %></p>
<p><%= f.text_field :email %></p>

<p><%= f.submit %></p>

<% end %>
